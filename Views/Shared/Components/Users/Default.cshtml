@model IEnumerable<LexiconLMS.Core.ViewModels.SystemUserViewModel>
@foreach (var user in Model)
{
    <tr>
        <td>@user.Name</td>
        <td>@user.Email</td>
        <td>@user.PhoneNumber</td>
        <td>
            <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Edit User</a>
        </td>
    </tr>
}