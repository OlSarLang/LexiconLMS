@model IEnumerable<SystemUserViewModel>
<ul class="list-unstyled" id="userlistul">
    @foreach (var user in Model)
    {
        <li>
            @user.Name <a asp-controller="Courses" asp-action="AddUserToCourse" asp-route-userId="@user.Id" asp-route-courseId="@user.CourseId">Add</a>
        </li>
    }
</ul>
