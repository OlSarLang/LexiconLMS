@model IEnumerable<SystemUserViewModel>
<ul class="list-unstyled" id="userlistul">
    @foreach (var user in Model)
    {
        <li>
            @user.Name
            <button class="btn btn-success float-right adduserbutton" data-userid="@user.Id" data-courseid="@user.CourseId">Add</button>
        </li>
    }
</ul>